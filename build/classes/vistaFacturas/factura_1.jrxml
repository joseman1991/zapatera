	<queryString>
		<![CDATA[select f.identificacion, f.numerofactura,r.razonsocial,f.concepto, a.descripcion, cantidad, Precio, (cantidad*precio*iva/100) as IVA, fechafactura,(cantidad*precio) as subtotal, ((cantidad*precio)+ (cantidad*precio*iva/100)) as total ,r.direccion
from factura f, articulos a, detallefactura dt, responsables r
where dt.codfactura=$P{codfact}  and f.codfactura=dt.codfactura and a.codarticulo = dt.codarticulo
and r.codtiporesponsable=f.codtiporesponsable and r.identificacion= f.identificacion;]]>
	</queryString>
